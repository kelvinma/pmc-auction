{
  "name": "webpack-react-websockets-client",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "prod": "npm run build && npm run serve",
    "build": "node server/deploy",
    "build-css": "node-sass-chokidar src/ -o src/",
    "serve": "NODE_ENV=production && npm start",
    "start": "PORT=8000 npm-run-all -p watch-css start-js",
    "start-js": "react-scripts start",
    "test": "jest --coverage",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-jest": "^22.1.0",
    "babel-preset-airbnb": "^2.2.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.24.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "jest": "^22.1.4",
    "fetch-mock": "^5.10.1",
    "node-sass-chokidar": "0.0.1",
    "npm-run-all": "^4.0.2",
    "prop-types": "^15.5.10",
    "react-scripts": "0.9.5",
    "react-test-renderer": "^16.0.0",
    "redux-mock-store": "^1.2.3",
    "redux-devtools": "^3.1.1"
  },
  "dependencies": {
    "bluebird": "^3.3.0",
    "event-emitter": "^0.3.4",
    "node-sass": "^3.4.2",
    "express": "^4.13.1",
    "file-loader": "^0.8.5",
    "firebase": "^2.4.0",
    "http-proxy": "^1.13.2",
    "immutable": "^3.7.6",
    "isomorphic-fetch": "^2.2.1",
    "json-load": "^1.0.1",
    "json-loader": "^0.5.4",
    "keymirror": "^0.1.1",
    "material-ui": "^0.20.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.5",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "react-tap-event-plugin": "^3.0.0",
    "redux": "^3.6.1",
    "redux-actions": "^0.9.1",
    "redux-devtools-dock-monitor": "^1.1.0",
    "redux-devtools-log-monitor": "^1.0.5",
    "redux-form": "^5.0.1",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.2.0",
    "url-loader": "^0.5.7"
  },
  "jest": {
    "verbose": true,
    "setupFiles": [
      "./setupTests.js"
    ],
    "collectCoverageFrom": [
      "src/**/**/*.{js}",
      "!src/app.js",
      "!src/stores/*",
      "!src/components/index.js"
    ],
    "coverageReporters": [
      "lcov"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 5,
        "functions": 5,
        "lines": 5,
        "statements": 5
      }
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "./fileMock.js",
      "\\.(css|scss|less)$": "identity-obj-proxy"
    }
  }
}
